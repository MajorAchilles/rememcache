{
  "name": "rememcache",
  "version": "1.0.6",
  "description": "A smart caching abstraction layer that uses Redis if available, otherwise falls back to an in-memory LRU cache.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MajorAchilles/rememcache.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest --run --environment node",
    "runner": "npm run build && node runner.js",
    "rate-runner": "npm run build && node runner-rate-limit.js",
    "redis-runner": "npm run build && node runner-redis.js",
    "redis-rate-runner": "npm run build && node runner-rate-redis.js",
    "test:coverage": "vitest --coverage",
    "lint": "eslint \"./src/**/*.{ts,tsx,js}\"",
    "format": "prettier --write .",
    "prepare": "npm run build"
  },
  "keywords": [
    "cache",
    "redis",
    "lru",
    "memory",
    "abstraction",
    "fallback"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "prettier": "^2.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.5.0",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "ioredis": "^5.8.2",
    "lru-cache": "^11.2.4"
  }
}
